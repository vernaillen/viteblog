    "debug": "^4.3.4",
    "eslint": "^7.32.0",
    "rollup": "^2.77.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "rollup-plugin-vue": "^6.0.0",
    "tsup": "^4.14.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-plugin-windicss": "^1.8.7",
